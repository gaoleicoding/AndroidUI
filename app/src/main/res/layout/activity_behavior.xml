<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openHeaderBehaviorActivity"
        android:text="HeaderBehaviorActivity"
        android:textAllCaps="false" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openGaodeBottomSheetActivity"
        android:text="GaodeBottomSheetActivity"
        android:textAllCaps="false" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openSetCarLocationActivity"
        android:text="SetCarLocationActivity"
        android:textAllCaps="false" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openBottomSheetDialogActivity"
        android:text="BottomSheetDialogActivity"
        android:textAllCaps="false" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openBottomSheetDialogActivity"
        android:text="BottomSheetDialogActivity"
        android:textAllCaps="false" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="openBottomSheetDialogActivity2"
        android:text="BottomSheetDialogActivity2"
        android:textAllCaps="false" />


</LinearLayout>